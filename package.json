{
  "name": "@jlab-enhanced/cell-toolbar",
  "version": "0.1.0",
  "description": "A cell toolbar for JupyterLab.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/fcollonval/jlab-enhanced-cell-toolbar",
  "bugs": {
    "url": "https://github.com/fcollonval/jlab-enhanced-cell-toolbar/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Frederic COLLONVAL",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcollonval/jlab-enhanced-cell-toolbar.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.2.6",
    "@jupyterlab/cells": "^2.2.7",
    "@jupyterlab/celltags": "^2.2.0",
    "@jupyterlab/docregistry": "^2.2.4",
    "@jupyterlab/notebook": "^2.2.7",
    "@jupyterlab/observables": "^3.2.5",
    "@jupyterlab/settingregistry": "^2.2.0",
    "@jupyterlab/ui-components": "^2.2.0",
    "@lumino/commands": "^1.12.0",
    "@lumino/disposable": "^1.4.3",
    "@lumino/widgets": "^1.16.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "1.16.4",
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
